{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DeepHealthCheck": {
    "HealthChecks": [
      {
        "ServiceName": "LogSeverity",
        "HealthCheckName": "LogSeverityHealthCheck",
        "FailureStatus": "Unhealthy",
        "ServiceLifetime": "Singleton",
        "TimeoutInSeconds": 30,
        "Tags": [],
        "Context": {
          "Disabled": false,
          "LogLevels": [
            {
              "LogLevel": "Critical",
              "MaxWarningCount": 1,
              "PeriodInMinutes": 3
            },
            {
              "LogLevel": "Error",
              "MaxWarningCount": 3,
              "PeriodInMinutes": 1
            }
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
